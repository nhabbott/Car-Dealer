<?XML version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 5.3//EN" "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">  

<hibernate-mapping>
    <class name="main.Listing" table="emp1000">
        <id name="id">
            <generator class="assigned"></generator>
        </id>

        <property name="vehicle"></property>
        <property name="price"></property>
        <property name="datePosted"></property>
        <property name="publishListing"></property>
    </class>
</hibernate-mapping>